<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rajkumar.mymap" >

    <permission
        android:name="com.example.rajkumar.mymap.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />
    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />
    <uses-permission android:name="com.example.rajkumar.mymapap.permission.MAPS_RECEIVE" />"
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/agency"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.rajkumar.mymap.ListActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.rajkumar.mymap.ProjectDetailActivity"
            android:label="Apartment Details" >
        </activity>
        <activity
            android:name="com.example.rajkumar.mymap.MapViewActivity"
            android:label="Location Details" >
        </activity>



        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyBGKgLPLcbY2n8IYf2QIsXzQNfBKvxm6zI" />
    </application>
</manifest>
